{
    "commands": {
        "joke": {
            "handler": "joke",
            "params": {
                "url": "https://icanhazdadjoke.com/",
                "headers": {
                    "Accept": "application/json"
                }
            }
        },
        "catfact": {
            "method": "GET",
            "url": "https://catfact.ninja/fact",
            "json_path": "fact"
        },
        "summary": {
            "method": "GET",
            "url": "https://linkstash.hsp-ec.xyz/api/summary",
            "json_path": "summary"
        },
        "quack": {
            "handler": "quack",
            "params": {
                "url": "https://random-d.uk/api/random"
            }
        },
        "meow": {
            "handler": "meow",
            "params": {
                "url": "https://api.thecatapi.com/v1/images/search"
            }
        },
        "deepfry": {
            "handler": "deepfry",
            "params": {
                "imagemagick_args": [
                    "-modulate",
                    "100,250,100",
                    "-contrast-stretch",
                    "0",
                    "-statistic",
                    "NonPeak",
                    "3",
                    "-sharpen",
                    "0x10",
                    "-implode",
                    "0.3"
                ]
            }
        }
    }
}